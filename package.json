{
    "name": "archestra-lang",
    "displayName": "ArchestrA Lang",
    "description": "Archestra Syntax highlighting.",
    "version": "0.1.1",
    "publisher": "RoanFourie",
    "repository": "https://github.com/RoanFourie/archestra-lang.git",
    "license": "Apache-2.0",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "tags": [
        "archestra",
        "syntax highlighting",
        "programming language"
    ],
    "branding": {
        "color": "rgb(18, 30, 33)",
        "theme": "dark"
    },
    "icons": {
        "default": "icon.png"
    },
    "contributes": {
        "languages": [{
            "id": "archestra",
            "aliases": ["ArchestrA", "archestra"],
            "extensions": [".archestra"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "archestra",
            "scopeName": "source.archestra",
            "path": "./syntaxes/archestra.tmLanguage.json"
        }]
    }
}